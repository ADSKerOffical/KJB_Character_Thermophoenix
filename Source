        Size = Vector3.new(600, 600, 600)
    })
    partTweenn2:Play()

sound = Instance.new("Sound", game.Workspace)
sound.SoundId = "rbxassetid://5379028763"
sound.Volume = 3
sound.PlaybackSpeed = 1
sound.Looped = false
sound:Play()
 task.wait(3)
k:Stop()
game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
            end
end)

